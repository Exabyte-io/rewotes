ns["data"] = 
            function setMpid(pathName) {
                var pathParts = pathName.split( '/' ).filter(Boolean)
                var mpid = pathParts.pop();
                var app = pathParts.pop();
                return mpid && app === "materials" ? mpid : null;
            }
            ;
</script>
