    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
